#include <iostream>
using namespace std;
int main()
{
float b, c, m, sum;
int b1, c1, m1;
cout << "Banana price - ";
cin >> b;
cout << "Coffee price - ";
cin >> c;
cout << "Ice cream price - ";
cin >> m;
cout << "Number banana?"<<endl;
cin >> b1;
cout<< "Number coffee?"<<endl;
cin >> c1;
cout << "Number ice cream?"<<endl;
cin >> m1;

sum = b * b1 + c * c1 + m * m1;
cout << sum;
    return 0;
}
